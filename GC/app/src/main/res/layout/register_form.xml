<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent">



			<RelativeLayout android:id="@+id/page_register_form_ek1"
				android:layout_marginTop="0dp"
				android:layout_marginStart="0dp"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				tools:ignore="MissingConstraints">

				<View
					android:id="@+id/_bg__home_page_ek2"
					android:layout_width="411dp"
					android:layout_height="731dp"
					android:layout_marginStart="0dp"
					android:layout_marginTop="0dp"
					android:background="@drawable/_bg__home_page_ek2_shape" />


				<EditText
					android:id="@+id/emailEditText"
					android:layout_width="274dp"
					android:layout_height="45dp"
					android:layout_marginTop="325dp"
					android:layout_marginStart="50dp"
					android:hint="email"
					android:inputType="textEmailAddress" />


				<EditText
					android:id="@+id/phoneEditText"
					android:layout_width="274dp"
					android:layout_height="45dp"
					android:layout_marginTop="371dp"
					android:layout_marginStart="50dp"
					android:hint="phone"
					android:inputType="textPhonetic" />

				<EditText
					android:id="@+id/newPasswordEditText"
					android:layout_width="274dp"
					android:layout_height="45dp"
					android:layout_marginTop="420dp"
					android:layout_marginStart="50dp"
					android:hint="new password"
					android:inputType="textPassword" />

				<EditText
					android:id="@+id/confirmPasswordEditText"
					android:layout_width="274dp"
					android:layout_height="45dp"
					android:layout_marginTop="465dp"
					android:layout_marginStart="50dp"
					android:hint="confirm Password"
					android:inputType="textPassword" />


				<Button android:id="@+id/register"
					android:textStyle="bold"
					android:textSize="22sp"
					android:layout_marginLeft="65dp"
					android:layout_marginTop="550dp"
					android:layout_width="232dp"
					android:layout_height="65dp"
					android:text="@string/register_string"
					 />


				<TextView android:id="@+id/already_have_an_account_login"
					android:textStyle="bold"
					android:textSize="18sp"
					android:textColor="@color/already_have_an_account_login_color"
					android:layout_marginLeft="80dp"
					android:layout_marginTop="630dp"
					android:layout_width="310dp"
					android:layout_height="45dp"
					android:text="@string/already_have_an_account_login_string"
					 />

				<EditText
					android:id="@+id/nameEditText"
					android:layout_width="274dp"
					android:layout_height="45dp"
					android:layout_marginStart="50dp"
					android:layout_marginTop="280dp"
					android:hint="name"
					android:inputType="textPersonName" />


				<ImageView android:id="@+id/pngtree_keep_our_city_clean_png_png_image_6099580_2"
					android:src="@drawable/pngtree_keep_our_city_clean_png_png_image_6099580_1"
					 android:layout_marginStart="120dp"
					android:layout_marginTop="100dp"
					android:layout_width="140dp"
					android:layout_height="140dp" />

			</RelativeLayout>
</androidx.constraintlayout.widget.ConstraintLayout>