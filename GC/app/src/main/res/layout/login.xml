<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent">


			<RelativeLayout android:id="@+id/page_login_ek1"
				android:layout_marginTop="0dp"
				android:layout_marginStart="0dp"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				tools:ignore="MissingConstraints">

				<View   android:id="@+id/_bg__home_page_ek2"
					android:background="@drawable/_bg__home_page_ek2_shape"
					android:layout_marginStart="0dp"
					android:layout_marginTop="0dp"
					android:layout_width="411dp"
					android:layout_height="731dp" />

				<TextView android:id="@+id/login_ek3"
					android:textSize="32sp"
					android:textColor="@color/login_ek3_color"
					android:layout_marginLeft="140dp"
					android:layout_marginTop="267dp"
					android:layout_width="114dp"
					android:layout_height="63dp"
					android:text="@string/login_ek3_string"
					 />

				<EditText
					android:id="@+id/emailEditText"
					android:layout_width="274dp"
					android:layout_height="45dp"
					android:layout_marginTop="374dp"
					android:layout_marginStart="50dp"
					android:hint="Email"
					android:inputType="textEmailAddress" />

				<EditText
					android:id="@+id/passwordEditText"
					android:layout_width="274dp"
					android:layout_height="45dp"
					android:layout_marginTop="424dp"
					android:layout_marginStart="50dp"
					android:hint="Password"
					android:inputType="textPassword" />


				<Button android:id="@+id/loginButton"
					android:textSize="24sp"
					android:textColor="@color/login_now_color"
					android:layout_marginLeft="102dp"
					android:layout_marginTop="520dp"
					android:layout_width="137dp"
					android:layout_height="58dp"
					android:text="login"
					 />

				<TextView android:id="@+id/don_t_have_an_account_signup"
					android:textSize="16sp"
					android:textColor="@color/login_ek3_color"
					android:layout_marginLeft="90dp"
					android:layout_marginTop="600dp"
					android:layout_width="227dp"
					android:layout_height="31dp"
					android:text="@string/don_t_have_an_account_signup_string"
					 />

				<TextView android:id="@+id/forget_password"
					android:textSize="16sp"
					android:textColor="@color/login_now_color"
					android:layout_marginLeft="200dp"
					android:layout_marginTop="470dp"
					android:layout_width="134dp"
					android:layout_height="31dp"
					android:text="@string/forget_password_string"
					 />

				<ImageView android:id="@+id/pngtree_keep_our_city_clean_png_png_image_6099580_1"
					android:src="@drawable/pngtree_keep_our_city_clean_png_png_image_6099580_1"
					 android:layout_marginStart="100dp"
					android:layout_marginTop="98dp"
					android:layout_width="154dp"
					android:layout_height="154dp" />

			</RelativeLayout>
</androidx.constraintlayout.widget.ConstraintLayout>